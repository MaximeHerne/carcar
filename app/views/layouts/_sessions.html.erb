<% if teacher_signed_in? %>
  <%= current_teacher.email %>
  <%= link_to 'Sign out', destroy_teacher_session_path, method: 'delete' %>
<% elsif learner_signed_in? %>
<%= current_learner.email %>
<%= link_to 'Sign out', destroy_learner_session_path, method: 'delete' %>
<% else %>
  <%= link_to 'Sign up (moniteur)', new_teacher_registration_path %>
  <%= link_to 'Sign up (learner)', new_learner_registration_path %>
<% end %>